{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 7 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/7"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "ext-curl": "*",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-xml": "*",
        "composer/installers": "^1.2",
        "composer/semver": "^1.4",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/preserve-paths": "^0.1",
        "drupal/admin_views": "^1.7",
        "drupal/adminimal_theme": "^1.26",
        "drupal/authcache": "^2.3",
        "drupal/auto_nodetitle": "^1.0",
        "drupal/bean": "^1.7",
        "drupal/blockreference": "^2.7",
        "drupal/breakpoints": "^1.6",
        "drupal/cacheexclude": "^2.3",
        "drupal/calendar": "^3.5",
        "drupal/captcha": "^1.6",
        "drupal/cer": "^3.0",
        "drupal/ckeditor": "^1.18",
        "drupal/ckeditor_media": "^2.0",
        "drupal/composer_autoloader": "^1.0",
        "drupal/contact_forms": "^1.9",
        "drupal/contextual_range_filter": "^1.7",
        "drupal/date_ical": "^3.9",
        "drupal/date_repeat_entity": "^2.0",
        "drupal/devel": "^1.7",
        "drupal/diff": "^3.4",
        "drupal/drupagram": "^1.3",
        "drupal/drupal": "^7.62",
        "drupal/editablefields": "^1.0",
        "drupal/editableviews": "^1.0",
        "drupal/email": "^1.3",
        "drupal/entity": "^1.9",
        "drupal/entity_view_mode": "^1.0",
        "drupal/entityreference": "^1.5",
        "drupal/entityreference_feeds": "^1.0",
        "drupal/entityreference_filter": "^1.7",
        "drupal/fast_404": "^1.5",
        "drupal/features": "^2.11",
        "drupal/feed_import": "^3.4",
        "drupal/feeds_tamper": "^1.2",
        "drupal/feeds_xpathparser": "^1.1",
        "drupal/fences": "^1.2",
        "drupal/field_collection_feeds": "^1.0",
        "drupal/field_group": "^1.6",
        "drupal/field_permissions": "^1.1",
        "drupal/file_entity": "^2.27",
        "drupal/filefield_paths": "^1.1",
        "drupal/google_analytics": "^2.6",
        "drupal/image_field_caption": "^2.3",
        "drupal/image_url_formatter": "^1.4",
        "drupal/imagecache_actions": "^1.11",
        "drupal/jquery_update": "^2.7",
        "drupal/link": "^1.7",
        "drupal/login_destination": "^1.4",
        "drupal/match_redirect": "^1.0",
        "drupal/mathjax": "^2.5",
        "drupal/media": "^2.24",
        "drupal/memcache": "^1.6",
        "drupal/migrate": "^2.11",
        "drupal/migrate_extras": "^2.5",
        "drupal/module_filter": "^2.2",
        "drupal/multiform": "^1.6",
        "drupal/navbar": "^1.7",
        "drupal/node_limit": "^1.0",
        "drupal/pathauto": "^1.3",
        "drupal/recaptcha": "^2.3",
        "drupal/redirect": "^1.0",
        "drupal/robotstxt": "^1.4",
        "drupal/seckit": "^1.11",
        "drupal/securepages": "^1.0",
        "drupal/simplesamlphp_auth": "^2.0",
        "drupal/simplified_menu_admin": "^1.0",
        "drupal/smtp": "^1.7",
        "drupal/strongarm": "^2.0",
        "drupal/title": "^1.0",
        "drupal/token_filter": "^1.1",
        "drupal/token_formatters": "^1.2",
        "drupal/transliteration": "^3.2",
        "drupal/twitter": "^5.11",
        "drupal/upgrade_status": "^1.0",
        "drupal/uuid_features": "^1.0",
        "drupal/views": "^3.23",
        "drupal/views_block_filter_block": "^1.0",
        "drupal/views_bulk_operations": "^3.5",
        "drupal/views_data_export": "^3.2",
        "drupal/views_data_export_html": "^1.1",
        "drupal/views_datasource": "^1.0",
        "drupal/views_distinct": "^1.0",
        "drupal/views_url_alias": "^1.0",
        "drupal/webform": "^4.20",
        "drupal/webform_matrix_component": "^4.28",
        "drupal/webform_template": "^4.0",
        "drupal/webform_validation": "^1.16",
        "drupal/wordpress_migrate": "^2.4",
        "drupal/wysiwyg": "^2.6",
        "drupal/xmlsitemap": "^2.6",
        "drush/drush": "^8.0",
        "symfony/filesystem": "~2.7|^3",
        "webflo/drupal-finder": "^1.0.0"
    },
    "conflict": {
        "drupal/core": "8.*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-create-project-cmd": [
            "DrupalProject\\composer\\ScriptHandler::removeInternalFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/": ["type:drupal-core"],
            "web/profiles/{$name}/": ["type:drupal-profile"],
            "web/sites/all/drush/{$name}/": ["type:drupal-drush"],
            "web/sites/all/libraries/{$name}/": ["type:drupal-library"],
            "web/sites/all/modules/contrib/{$name}/": ["type:drupal-module"],
            "web/sites/all/themes/contrib/{$name}/": ["type:drupal-theme"]
        },
        "patches": {
            "cweagans/composer-patches": {
                "Call the preserve paths hooks" : "https://github.com/SebCorbin/composer-patches/commit/1cb9bacba51f8bba0c5f3f322c3bb61e4ceae974.patch"
            }
        },
        "preserve-paths": [
            "web/sites"
        ]
    }
}
